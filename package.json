{
  "name": "boclips-js-security",
  "version": "0.0.0-SNAPSHOT",
  "description": "Boclips Keycloak Wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "audit": "npm audit --audit-level critical --production",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "lint": "tslint -c tslint.json -p . --fix",
    "build": "webpack && tsc",
    "compile": "tsc -p tsconfig.all.json --noEmit",
    "demo:build": "tsc --build demo/module/tsconfig.json && webpack --config demo/module/webpack.config.js",
    "demo": "./node_modules/.bin/webpack-dev-server --config demo/module/webpack.config.js",
    "demo:autologin": "./node_modules/.bin/webpack-dev-server --config demo/autologin/webpack.config.js",
    "demo:unpkg": "serve -l 8081  demo/unpkg",
    "e2e": "$(npm bin)/cypress open",
    "e2e:ci": "$(npm bin)/cypress run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "keycloak-js": "9.0.3"
  },
  "peerDependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.0.7",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "cypress": "^4.11.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^25.1.0",
    "prettier": "^2.1.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.1",
    "serve": "^11.3.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "2.3.x",
    "typescript": "^3.0.3",
    "uuid": "^8.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/knowledgemtion/boclips-js-security.git"
  }
}
